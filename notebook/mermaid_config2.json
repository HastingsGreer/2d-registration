{
    "model": {
        "deformation": {
            "compute_similarity_measure_at_low_res": false,
            "use_map": true
        },
        "registration_model": {
            "forward_model": {
                "number_of_time_steps": 10,
                "solver": "dopri5",
                "adjoin_on": true,
                "rtol": 1e-5,
                "atol": 1e-5,
                "smoother": {
                    "multi_gaussian_stds": [
                        0.025,
                        0.05,
                        0.1,
                        0.15,
                        0.2
                    ],
                    "multi_gaussian_weights": [
                        0.06666666666666667,
                        0.13333333333333333,
                        0.19999999999999998,
                        0.26666666666666666,
                        0.3333333333333333
                    ],
                    "type": "multiGaussian"
                },
                "smoother_for_forward": {
                    "type": "multiGaussian"
                }
            },

            "load_velocity_from_forward_model":true,
            "spline_order": 1,
            "type": "lddmm_shooting_map",
            "env":{
                "get_momentum_from_external_network":true,
                "use_odeint":true,
                "reg_factor":1.0
                },
            "shooting_vector_momentum":{
                "use_velocity_mask_on_boundary":false
            }
        }
    }
}
